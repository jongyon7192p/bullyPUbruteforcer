<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="io.github.jgcodes.bitfs0x.querier.dialog.LoginFormController"
            prefHeight="400.0" prefWidth="600.0"
            hgap="5" vgap="5">
  <Label GridPane.columnIndex="0" GridPane.rowIndex="0"
    text="Database URL: "/>
  <TextField GridPane.columnIndex="1" GridPane.rowIndex="0"
    promptText="Enter database URL" fx:id="urlField"/>
  <Label GridPane.columnIndex="0" GridPane.rowIndex="1"
    text="Username: "/>
  <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"
    promptText="Enter username" fx:id="userField"/>
  <Label GridPane.columnIndex="0" GridPane.rowIndex="2"
    text="Password: "/>
  <PasswordField GridPane.columnIndex="1" GridPane.rowIndex="2"
    promptText="Enter password" fx:id="passwordField"/>
</GridPane>
